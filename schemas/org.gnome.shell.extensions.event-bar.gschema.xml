<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.event-bar" path="/org/gnome/shell/extensions/event-bar/">
    <!-- See also: https://docs.gtk.org/glib/gvariant-format-strings.html -->
    <key name="which-panel" type="i">
      <default>1</default>
    </key>
    <key name="event-length" type="i">
      <default>20</default>
    </key>
    <key name="show-duration" type="b">
      <default>false</default>
    </key>
    <key name="show-time-before-event" type="b">
      <default>true</default>
    </key>
    <key name="show-time-of-event" type="b">
      <default>false</default>
    </key>
    <key name="show-event-icon" type="b">
      <default>true</default>
    </key>
    <key name="show-event-name" type="b">
      <default>true</default>
    </key>
    <key name="show-next-event-during-meeting" type="b">
      <default>true</default>
    </key>

  </schema>
</schemalist>
